{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "repositories": {
        "moloniapi": {
            "type": "path",
            "url": "packages/digitaldevlx/moloniapi",
            "options": {
                "symlink": true
            }
        }
    },
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.1.3",
        "ext-json": "*",
        "barryvdh/laravel-dompdf": "^0.8.3",
        "barryvdh/laravel-translation-manager": "^0.5.2",
        "craftsys/msg91-laravel-notification-channel": "^0.3.1",
        "digitaldev-lx/moloni-api": "dev-master",
        "doctrine/dbal": "2.6",
        "fideloper/proxy": "^4.0",
        "google/apiclient": "^2.10",
        "google/recaptcha": "~1.1",
        "grimzy/laravel-mysql-spatial": "3.0",
        "guzzlehttp/guzzle": "^6.3",
        "intervention/image": "^2.3",
        "laravel/cashier": "^12.2",
        "laravel/framework": "^7.0",
        "laravel/helpers": "^1.2",
        "laravel/nexmo-notification-channel": "^2.1",
        "laravel/socialite": "^5.2",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^2.0",
        "macellan/laravel-zip": "^1.0",
        "mews/purifier": "^3.3",
        "nexmo/laravel": "^2.0",
        "paypal/rest-api-sdk-php": "^1.14",
        "razorpay/razorpay": "2.*",
        "santigarcor/laratrust": "5.2.*",
        "shetabit/visitor": "^3.1",
        "stripe/stripe-php": "^7.68",
        "yajra/laravel-datatables-oracle": "^9.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.6",
        "filp/whoops": "^2.0",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^4.1",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/",
            "DigitaldevLx\\MoloniApi\\": "packages/digitaldevlx/moloniapi/src"
        },
        "files": [
            "app/Helper/start.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
