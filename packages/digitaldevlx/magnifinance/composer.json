{
    "name": "digitaldev-lx/magnifinance",
    "description": "Magnifinance Api Laravel Package",
    "keywords": [
        "digitaldev-lx",
        "magnifinance"
    ],
    "homepage": "https://github.com/digitaldev-lx/magnifinance",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Paulo Serrano",
            "email": "geral@digitaldev.pt",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0|^8.1",
        "illuminate/support": "^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "DigitaldevLx\\Magnifinance\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DigitaldevLx\\Magnifinance\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "DigitaldevLx\\Magnifinance\\MagnifinanceServiceProvider"
            ],
            "aliases": {
                "Magnifinance": "DigitaldevLx\\Magnifinance\\MagnifinanceFacade"
            }
        }
    }
}
